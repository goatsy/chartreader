{
  "name": "chartreader",
  "version": "1.0.0",
  "description": "Read Charts from mtv.de and get all the new entries and display them",
  "main": "index.js",
  "scripts": {
    "remove_output": "./node_modules/rimraf/bin.js output",
    "charts": "./node_modules/.bin/wdio wdio.conf.js --spec ./specs/charts.spec.js",
    "charts_midweek": "./node_modules/.bin/wdio wdio.conf.js --spec ./specs/midweeks.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goatsy/chartreader.git"
  },
  "engines": {
    "node": "10.15.1"
  },
  "keywords": [
    "webdriverio",
    "nodejs",
    "javascript"
  ],
  "author": "goatsy (mzehetmeier)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/goatsy/chartreader/issues"
  },
  "homepage": "https://github.com/goatsy/chartreader#readme",
  "devDependencies": {
    "@wdio/cli": "^5.6.2",
    "@wdio/dot-reporter": "^5.6.0",
    "@wdio/local-runner": "^5.6.2",
    "@wdio/mocha-framework": "^5.6.0",
    "@wdio/selenium-standalone-service": "^5.6.0",
    "@wdio/spec-reporter": "^5.6.0",
    "@wdio/sync": "^5.6.0",
    "rimraf": "^2.6.3",
    "webdriverio": "^5.6.2"
  }
